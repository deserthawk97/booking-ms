pipeline {
	agent any
	options{ buildDiscarder(logRotator(numToKeepStr: '5', artifactNumToKeepStr: '5')) }
	tools {
	 maven 'apache-maven-3.9.10'
	 java 'java21'
	 }
	 stages {
		stage(' Java version' ) {
			steps {
			echo "Hello This is java"
			sh java --version
			}
		}
	 stages {
		stage(' Maven version' ) {
			steps {
			echo "Hello This is Maven"
			sh maven --version
			}
		}
		stage(' Date ' ) {
			steps {
			echo "This is Date stage"
			sh date
			}
		}
	 }
}
